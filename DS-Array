#include <stdio.h>
int main()
{
    int size;
    scanf("%d", &size);
    int arr[size];
    for(int i=0; i<size; i++)
    {
        scanf("%d", &arr[i]);
    }
    int brr[size];
    for(int i=0; i<size; i++)
    {
        brr[i]=arr[(size-1)-i];
    }
    for(int i=0; i<size; i++)
    {
        printf("%d ", brr[i]);
    }
    return 0;
}
